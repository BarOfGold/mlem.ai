  "apply": {
    "args": {
      "args": [
        {
          "decl": "model",
          "help": "Path to model object [required]"
        },
        {
          "decl": "data",
          "help": "Path to data object [required]"
        }
      ],
      "impls": null,
      "impl_metavar": null,
      "subcommands": null
    },
    "options": [
      {
        "decl": "-p, --project TEXT",
        "help": "Path to MLEM project [default: (none)]"
      },
      {
        "decl": "--rev TEXT",
        "help": "Repo revision to use [default: (none)]"
      },
      {
        "decl": "-o, --output TEXT",
        "help": "Where to store the outputs."
      },
      {
        "decl": "-m, --method TEXT",
        "help": "Which model method is to be applied [default: predict]"
      },
      {
        "decl": "--data-project, --dr TEXT",
        "help": "Project with data"
      },
      {
        "decl": "--data-rev TEXT",
        "help": "Revision of data"
      },
      {
        "decl": "-i, --import",
        "help": "Try to import data on-the-fly"
      },
      {
        "decl": "--import-type, --it TEXT",
        "help": "Specify how to read data file for import. Available types: ['pandas', 'pickle']"
      },
      {
        "decl": "-b, --batch_size INTEGER",
        "help": "Batch size for reading data in batches."
      },
      {
        "decl": "--index / --no-index",
        "help": "Whether to index output in .mlem directory"
      },
      {
        "decl": "-e, --external",
        "help": "Save result not in .mlem, but directly in project"
      },
      {
        "decl": "--json",
        "help": "Output as json"
      },
      {
        "decl": "--help",
        "help": "Show this message and exit."
      }
    ],
    "usage": "Usage: mlem apply [options] model data",
    "doc": "Apply a model to data. The result will be saved as a MLEM object to `output` if\nprovided. Otherwise, it will be printed to `stdout`."
  },
